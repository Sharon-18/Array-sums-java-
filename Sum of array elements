import java.util.Scanner;

class T {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();
        int total = 0;

        int[] arr = new int[n];
        
        for (int i = 0; i < arr.length; i++) {
            arr[i] = sc.nextInt(); // Missing this line to take array input
            total = total + arr[i];
        }
        
        System.out.print(total);
    }
}
