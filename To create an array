import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int m=sc.nextInt();
        int b[] = new int[m];
        for(int i=0;i<b.length;i++){
           b[i]=sc.nextInt();
        }
        for(int i=0;i<b.length;i++){
            System.out.print(b[i]);
            
            }
        }
            
        }
